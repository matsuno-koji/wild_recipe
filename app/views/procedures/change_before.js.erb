<% if @file_exist %>
  $('div[name="number[<%= @procedure.number %>]"]').html( "<%= escape_javascript( @procedure.content) %>" );
  $('#textarea<%= @procedure.number%>').val( "<%= escape_javascript( @procedure.content) %>" );
  $('div[name="number[<%= @previous_procedure.number %>]"]').html( "<%= escape_javascript( @previous_procedure.content )%> ");
  $('#textarea<%= @previous_procedure.number%>').val( "<%= escape_javascript( @previous_procedure.content) %>" );
<% end %>