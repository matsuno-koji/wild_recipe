<% if @file_exist %>
  $('div[name="number[<%= @procedure.number %>]"]').html( "<%= escape_javascript(@procedure.content) %>" );
  $('#textarea<%= @procedure.number%>').val( "<%= escape_javascript( @procedure.content) %>" );
  $('div[name="number[<%= @next_procedure.number %>]"]').html( "<%= escape_javascript(@next_procedure.content )%> ");
  $('#textarea<%= @next_procedure.number%>').val( "<%= escape_javascript( @next_procedure.content) %>" );
<% end %>